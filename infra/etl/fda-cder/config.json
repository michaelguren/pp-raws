{
  "dataset": "fda-cder",
  "source_url": "https://www.accessdata.fda.gov/cder/ndctext.zip",
  "description": "FDA CDER National Drug Code Directory with Products and Packages",
  "data_size_category": "medium",
  "date_format": "MM/dd/yyyy",
  "schedule": {
    "enabled": false,
    "expression": "cron(0 6 * * ? *)"
  },
  "file_table_mapping": {
    "product.txt": "products",
    "package.txt": "packages"
  }
}